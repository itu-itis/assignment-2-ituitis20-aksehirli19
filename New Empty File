function insertEntry(sporId, takimId){
  var spor = document.getElementById(sporId);
  var takim = document.getElementById(takimId);
  
  
  var sporTable = document.getElementById("sporTable");
  var newRow = document.createElement("TR");
  newRow.setAttribute("id","myRow");
  sporTable.appendChild(newRow);
  
  var sporTd = document.createElement("TD");
  var sporNode = document.createTextNode(spor.value);
  sporTd.appendChild(sporNode);
  newRow.appendChild(sporTd);
 
  var takimTd = document.createElement("TD");
  var takimNode = document.createTextNode(takim.value);
  takimTd.appendChild(takimNode);
  newRow.appendChild(takimTd);
  

  }
